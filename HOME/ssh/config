
# For github jchakir
Host github-jchakir
	Identityfile ~/.ssh/id_ed25519-github-jchakir


# for all githubs
Host github*
	HostName github.com
	User git
	IdentityFile ~/.ssh/id_ed25519-github


# For jchakir.com vps
Host *jchakir.com
	ForwardX11 yes


# for all githubs
Host *
	AddKeysToAgent yes
	IdentitiesOnly yes
	IdentityFile ~/.ssh/id_ed25519

